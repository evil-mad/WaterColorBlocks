<block-definition s="WCB get paint %'color'" type="command" category="pen">
<header/>
<code/>
<inputs>
<input type="%n">
0
<options>
0 (black)=0 1 (red)=1 2 (orange)=2 3 (yellow)=3 4 (green)=4 5 (blue)=5 6 (violet)=6 7 (brown)=7
</options>
</input>
</inputs>
<script>
<custom-block s="WCB do %txt">
<block s="reportJoinWords">
<list>
<l>tool/color/</l>
<block var="color"/>
</list>
</block>
</custom-block>
<block s="setBrightness">
<l>100</l>
</block>
<block s="doIf">
<block s="reportEquals">
<block var="color"/>
<l>0</l>
</block>
<script>
<block s="setBrightness">
<l>0</l>
</block>
<block s="setHue">
<l>0</l>
</block>
</script>
</block>
<block s="doIf">
<block s="reportEquals">
<block var="color"/>
<l>1</l>
</block>
<script>
<block s="setHue">
<l>0</l>
</block>
</script>
</block>
<block s="doIf">
<block s="reportEquals">
<block var="color"/>
<l>2</l>
</block>
<script>
<block s="setHue">
<l>8</l>
</block>
</script>
</block>
<block s="doIf">
<block s="reportEquals">
<block var="color"/>
<l>3</l>
</block>
<script>
<block s="setHue">
<l>14</l>
</block>
</script>
</block>
<block s="doIf">
<block s="reportEquals">
<block var="color"/>
<l>4</l>
</block>
<script>
<block s="setHue">
<l>30</l>
</block>
</script>
</block>
<block s="doIf">
<block s="reportEquals">
<block var="color"/>
<l>5</l>
</block>
<script>
<block s="setHue">
<l>60</l>
</block>
</script>
</block>
<block s="doIf">
<block s="reportEquals">
<block var="color"/>
<l>6</l>
</block>
<script>
<block s="setHue">
<l>70</l>
</block>
</script>
</block>
<block s="doIf">
<block s="reportEquals">
<block var="color"/>
<l>7</l>
</block>
<script>
<block s="setBrightness">
<l>50</l>
</block>
<block s="setHue">
<l>0</l>
</block>
</script>
</block>
</script>
</block-definition>
